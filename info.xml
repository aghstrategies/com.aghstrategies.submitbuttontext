<?xml version="1.0"?>
<extension key="com.aghstrategies.submitbuttontext" type="module">
  <file>submitbuttontext</file>
  <name>Submit Button Text</name>
  <description>Creates a setting to change the wording of the submit buttons for events, membership and contributions pages.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alice Frumin</author>
    <email>alice@aghstrategies.com</email>
  </maintainer>
  <urls>
    <url desc="documentation">https://github.com/aghstrategies/com.aghstrategies.submitbuttontext</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-01-03</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Submitbuttontext</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
